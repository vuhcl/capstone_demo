    class NonUniformRandom(object):
        """
        Class to perform weighted sampling to replace function in NumPy
        """
        def __init__(self, list_of_values, list_of_probabilities):
            """
            expects a list of all values and a list of their
            corresponding probabilities
            """
            self.values = list_of_values
            self.probabilities = list_of_probabilities

        def pick(self):
            """
            return a random value taking into account the probabilities
            """
            import random
            r = random.uniform(0, sum(self.probabilities))
            s = 0.0
            for k, w in zip(self.values, self.probabilities):
                s += w
                if r < s: return k
            return k

